{
  "manifest_version": 2,
  "name": "chrome-linkedin-oauth",
  "short_name": "chrome-linkedin-oauth",
  "description": "Description for chrome-linkedin-oauth",

  "version": "0.0.1.0",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },

  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "key":"",
    
  "permissions": [
      "identity", 
      "https://www.linkedin.com/*", 
      "https://api.linkedin.com/*"
  ]
}
